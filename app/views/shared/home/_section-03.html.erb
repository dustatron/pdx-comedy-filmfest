

<div class="row">
<div class="video--first-place"><i class="fas fa-award"></i> First Place winners</div>

<% @first.each do |first| %>
    <div class="col-md-4">
        <%= render "shared/player", submission: first %>
    </div>
<%end%>

</div>
<div class="row">
<span class="video--award-button"><%= link_to "All 1st Winners", static_pages_awards_path(:category => 'First Place', :place => '1st') %></span>
</div>


