<!DOCTYPE html>
<html>
  <head>
    <title>PDX Funniest Videos</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <!-- Fixed navbar -->
    <nav class="navbar navbar-inverse navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <%= link_to "PDX Funniest Videos", static_pages_home_path, class: "navbar-brand" %>
        </div>
        <div id="navbar" class="navbar-collapse collapse navbar-right">
          <ul class="nav navbar-nav">
            <li class="active"><%= link_to "Home", static_pages_home_path %></li>
            <li><%= link_to "About", static_pages_about_path %></li>
            <li><%= link_to "Submit", static_pages_submit_path %></li>
            <li><%= link_to "Help", static_pages_help_path %></li>

            
        </div><!--/.nav-collapse -->
      </div>
    </nav>
    <!-- Body Body -->
    <div class="container">
      <div class="jumbotron">
        <%= yield %>
        <a class="btn btn-primary btn-lg" href="#" role="button">Learn more</a>
    
    </div>
    </div>
  </body>
</html>
